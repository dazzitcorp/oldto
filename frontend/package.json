  },
  "devDependencies": {
    "@plasmohq/parcel-transformer-inject-env": "^0.2.3",
    "eslint": "^2.10.2",
    "eslint-plugin-import": "^1.8.0",
    "events": "^3.3.0",
    "http-proxy-middleware": "^2.0.6",
    "parcel": "^2.8.2",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "process": "^0.11.10",
    "siimple": "^4.3.1"
  },
  "dependencies": {
    "clipboard": "^2.0.0",
    "mapbox-gl-compare": "^0.2.0",
    "underscore": "^1.8.3"
  }
}
