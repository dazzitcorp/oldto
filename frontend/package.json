{
  "name": "oldto",
  "license": "ISC",
  "version": "1.0.0",
  "browserslist": "defaults",
  "source": "src/index.html",
  "staticFiles": {
    "staticPath": "src/static"
  },
  "scripts": {
    "build": "parcel build --dist-dir dist",
    "lint": "eslint src/js/*.js",
    "siimple": "npx siimple -c ./siimple.config.mjs -o ./src/styles/main.css",
