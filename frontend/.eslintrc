{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "google": false,
    "isKiosk": false
  },
  "rules": {
    "eqeqeq": 2,
    "no-console": 0,
    "sort-imports": "error"
  }
}
